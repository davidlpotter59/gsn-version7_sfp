                     }
                }
            }
          else
            {
              if axis_master_premium:company_id       <> prsmaster:company_id or
                 axis_master_premium:policy_no        <> prsmaster:policy_no or
                 axis_master_premium:pol_year         <> prsmaster:pol_year or
