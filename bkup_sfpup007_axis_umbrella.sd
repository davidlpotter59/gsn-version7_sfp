procedure check_for_deleted_location

--this procedure will access the axis_master file and check to see if the prior endorsement
--  to the current endorsement has any missing locations/coverages.

BEGIN
local file axis_master_prior = alias for axis_master;
local file sfpname_current = alias for sfpname;
local file sfpmaster_current = alias for sfpmaster;

local signed ascii number l_difference[9] = 0,
                          l_save_prem_no[4],
                          l_prior_end_sequence[4],
                          l_save_build_no[4];

local array [10] of unsigned ascii number l_saved_subline[4];
local unsigned ascii number l_looping[4];
string l_policy_suffix[3];
local unsigned ascii number L_current_trans_code[2];

wdate l_trans_date,
      l_policy_eff_date,
      l_trans_eff_date,
      l_trans_exp_date,
      l_policy_exp_date;

access axis_master_loop,
   set axis_master_loop:policy_no        = l_hold_policy_no,
       axis_master_loop:pol_year         = l_hold_pol_year,
       axis_master_loop:end_sequence     = l_hold_end_sequence, generic      --use the current endorsement to access when the endorsement in the the same as the l_endorsement and the pol_year and policy is the same

--I need to find the previous end_sequence.  it will not always be end_sequence -1 so If I go previous here I can save the previous end_sequence and then access it with the correct end_sequence.
--I only need to check the prior endorsment no others are needed because this is done every time a policy endorsement is finished.

while axis_master_loop:policy_no        = l_hold_policy_no and
      axis_master_loop:pol_year         = l_hold_pol_year and
      axis_master_loop:end_sequence     = l_hold_end_sequence
  {
    previous axis_master_loop
  }

if axis_master_loop:policy_no        = l_hold_policy_no and   --make sure there is a prior to add something from.
   axis_master_loop:pol_year         = l_hold_pol_year then
  {
    l_prior_end_sequence = axis_master_loop:end_sequence    --need to save this to use later so I know the end_sequence to check with.

    --I have to loop from all the location of the prior record and compare it to the one that was just entered to see if there are any deleted location
    --If I find something that is not in the current endorsement then I need to add the prior record into the current and put it as a negative premium

    access axis_master_current,
       set axis_master_current:policy_no    = l_hold_policy_no,
           axis_master_current:pol_year     = l_hold_pol_year,
           axis_master_current:end_sequence = l_hold_end_sequence, generic

    access sfpname_current,
       set sfpname_current:policy_no    = l_hold_policy_no,
           sfpname_current:pol_year     = l_hold_pol_year,
           sfpname_current:end_sequence = l_hold_end_sequence, generic

    access sfpmaster_current,
       set sfpmaster_current:policy_no    = l_hold_policy_no,
           sfpmaster_current:pol_year     = l_hold_pol_year,
           sfpmaster_current:end_sequence = l_hold_end_sequence, generic

    --I need to get the dates of the current endorsement and save them so I can then add them back in when I add in the missing location with the current endorsments dates.

    if axis_master_current:policy_no    = l_hold_policy_no and
       axis_master_current:pol_year     = l_hold_pol_year and
       axis_master_current:end_sequence = l_hold_end_sequence then
      {
        l_current_trans_code  = axis_master_current:trans_code
        l_trans_date          = axis_master_current:trans_date
        l_policy_eff_date     = axis_master_current:policy_eff_date
        l_policy_exp_date     = axis_master_current:policy_exp_date
        l_trans_eff_date      = axis_master_current:trans_eff_date
        l_trans_exp_date      = axis_master_current:trans_exp_date
        l_policy_suffix       = axis_master_current:policy_suffix
      }

    access axis_master_prior,
       set axis_master_prior:policy_no        = l_hold_policy_no,
           axis_master_prior:pol_year         = l_hold_pol_year,
           axis_master_prior:end_sequence     = l_prior_end_sequence, generic

    while axis_master_prior:policy_no        = l_hold_policy_no and
          axis_master_prior:pol_year         = l_hold_pol_year and
          axis_master_prior:end_sequence     = l_prior_end_sequence
      {

            access axis_master_current,
               set axis_master_current:policy_no        = axis_master_prior:policy_no,
                   axis_master_current:pol_year         = axis_master_prior:pol_year,
                   axis_master_current:end_sequence     = l_hold_end_sequence,
                   axis_master_current:prem_no          = axis_master_prior:prem_no,
                   axis_master_current:build_no         = axis_master_prior:build_no,
                   axis_master_current:line_of_business = axis_master_prior:line_of_business,
                   axis_master_current:lob_subline      = axis_master_prior:lob_subline, generic

        if axis_master_current:policy_no        <> axis_master_prior:policy_no or
           axis_master_current:pol_year         <> axis_master_prior:pol_year or
           axis_master_current:end_sequence     <> l_hold_end_sequence or
           axis_master_current:prem_no          <> axis_master_prior:prem_no or
           axis_master_current:build_no         <> axis_master_prior:build_no or
           axis_master_current:line_of_business <> axis_master_prior:line_of_business or
           axis_master_current:lob_subline      <> axis_master_prior:lob_subline then
          {
            l_premium = axis_master_prior:annual_premium
            if sfpmaster_current:trans_eff <> sfpname_current:eff_date then
              {
                I_EFF_DATE = Dateadd(l_trans_exp_date,0,-1)
                I_SHORT_RATE = SFPMASTER_current:SHORT_RATE
                I_TOTAL_DAYS = I_EFF_DATE - l_TRANS_EXP_date
                I_CX_DAYS = I_EFF_DATE - l_TRANS_EFF_date
                Include "prorata.pro"
                l_difference = l_premium * I_pro_rata
                l_premium = l_difference
              }
            l_premium = l_premium * -1
            if axis_master_prior:mark_as_deleted_location not one of 1 then   --this will eliminate adding the location to every subsquent endorsement
              {
                add axis_master_current
                  begin
                    axis_master_current:POLICY_NO                = axis_master_prior:policy_no
                    axis_master_current:POL_YEAR                 = axis_master_prior:pol_year
                    axis_master_current:END_SEQUENCE             = l_hold_end_sequence
                    axis_master_current:PREM_NO                  = axis_master_prior:prem_no
                    axis_master_current:BUILD_NO                 = axis_master_prior:build_No
                    axis_master_current:lob_subline              = axis_master_prior:lob_subline
                    axis_master_current:line_of_business         = axis_master_prior:line_of_business
                    axis_master_current:SUB_CODE                 = axis_master_prior:sub_code
                    axis_master_current:record_rdf               = axis_master_prior:record_rdf/raw
                    axis_master_current:trans_date               = l_trans_date
                    axis_master_current:trans_code               = l_current_trans_code
                    axis_master_current:policy_eff_date          = l_policy_eff_date
                    axis_master_current:trans_eff_date           = l_trans_eff_date
                    axis_master_current:trans_exp_date           = l_trans_exp_date
                    axis_master_current:save_current_premium     = l_premium
                    axis_master_current:premium                  = l_Premium
                    axis_master_current:annual_premium           = axis_master_prior:annual_premium
                    axis_master_current:accounting_date          = l_ending_date
                    axis_master_current:mark_as_deleted_location = 1
                    axis_master_current:coverage_exposure        = l_exposure
                    axis_master_current:iso_class_code           = axis_master_prior:iso_class_code
                    axis_master_current:iso_cf_year_of_construction   = "N/A"
                    axis_master_current:iso_bop_year_of_construction  = "N/A"
                  END
              }
          }
        next axis_master_prior
      }
  } --end of the if
END

procedure check_for_endorsement

BEGIN
  local unsigned ascii number l_previous_end_sequence[4];
  local signed ascii number l_previous_total[9];
  local signed ascii number l_diff_total[9];
  l_save_premium = 0

  access sfpmaster_check_endorsement,
     set sfpmaster_check_endorsement:policy_no    = l_policy_no,
         sfpmaster_check_endorsement:pol_year     = l_pol_year,
         sfpmaster_check_endorsement:end_sequence = l_end_sequence, generic

  if sfpmaster_check_endorsement:trans_code one of 11,12,13,15,16 then
    {  --check if prior endorsement is there
      l_previous_end_sequence = l_end_sequence - 1
      access axis_master_previous,
         set axis_master_previous:policy_no        = l_policy_no,
             axis_master_previous:pol_year         = l_pol_year,
             axis_master_previous:end_sequence     = l_previous_end_sequence,
             axis_master_previous:prem_no          = l_prem_no,
             axis_master_previous:build_no         = l_build_no,
             axis_master_previous:line_of_business = l_line_of_business,
             axis_master_previous:lob_subline      = l_lob_subline,
             axis_master_previous:line_type        = l_line_type,
             axis_master_previous:sub_code         = l_sub_code, exact

      if axis_master_previous:policy_no        = l_policy_no and
         axis_master_previous:pol_year         = l_pol_year and
         axis_master_previous:end_sequence     = l_previous_end_sequence and
         axis_master_previous:prem_no          = l_prem_no and
         axis_master_previous:build_no         = l_build_no and
         axis_master_previous:line_of_business = l_line_of_business and
         axis_master_previous:lob_subline      = l_lob_subline and
         axis_master_previous:line_type        = l_line_type and
         axis_master_previous:sub_code         = l_sub_code then
        {
          l_previous_total = 0
          l_previous_total = axis_master_previous:annual_premium
          l_save_premium = l_premium
          l_diff_total = l_premium - l_previous_total     --take current_premium - previous end_sequence premium to get the current_premium to add to axis_master.
          l_annual_premium = l_premium
          if sfpmaster_check_endorsement:trans_code one of 11 then
            {
              if sfpmaster_check_endorsement:trans_eff = sfpname:eff_date then
                {
                  l_annual_premium = l_annual_premium * -1
                  l_premium = l_premium * -1
                }
              else
                {
                  do pro_rata
                  l_diff_total = l_annual_premium * I_pro_rata
                  l_premium = l_diff_total * -1
                  l_annual_premium = l_annual_premium * -1
                }
            }
          else
            if sfpmaster_check_endorsement:trans_code one of 16 then
              {
                if sfpmaster_check_endorsement:trans_eff = sfpname:eff_date then
                  {
                    l_premium = l_premium
                    l_annual_premium = l_annual_premium
                  }
                else
                  {
                    do pro_rata
                    l_diff_total = l_annual_premium * I_pro_rata
                    l_annual_premium = l_annual_premium
                    l_premium = l_diff_total
                  }
              }
            else
              if l_diff_total <> 0 then
                {
                  if sfpmaster_check_endorsement:trans_eff <> sfpname:eff_date then
                    {
                      do pro_rata
                      l_diff_total = l_diff_total * I_pro_rata
                    }
                  l_premium = l_diff_total
                }
        }
      else   --was not found on prevous endorsement and need to check date to see if pro_rata is needed else just use l_premium
        {
          if sfpmaster_check_endorsement:trans_eff <> sfpname:eff_date then
            {
              do pro_rata
              l_premium = l_premium  * I_pro_rata
            }
        }
    }

END

procedure write_difference_in_premium

BEGIN
  -- L_premium is the total premium of the current subline and endorsement.
  signed ascii number l_new_premium_to_add[9],
                      l_save_annual_prior[9],
                      l_current_premium_to_add[9];


  access axis_master_loop,
     set axis_master_loop:policy_no        = l_policy_no,
         axis_master_loop:pol_year         = l_pol_year,  generic      --beginning of the current policy and pol_year

  while axis_master_loop:policy_no    = l_policy_no and
        axis_master_loop:pol_year     = l_pol_year
    {
      if axis_master_loop:prem_no     = l_prem_no and
         axis_master_loop:build_No    = l_build_no and
         axis_master_loop:lob_subline = l_lob_subline and
         l_line_of_business one of 7 then
        {
          l_current_premium_to_add = l_current_premium_to_add + axis_master_loop:premium    --was just premium
          l_save_annual_prior = axis_master_loop:annual_premium
        }
      next axis_master_loop
    }  --now I should have the correct total for this endorsement now I need to compare it to the the premium I am adding.

if sfpmaster:trans_eff <> sfpname:eff_date then --prorat
  {
    if l_save_annual_prior = l_annual_premium then
      {
        l_premium = 0
        l_save_premium = 0
      }
    else
      if l_premium >= l_current_premium_to_add then      --  l_current_premium_to_add >= l_premium then
        {
          l_new_premium_to_add = l_premium - l_current_premium_to_add
          l_save_premium = l_premium
          l_premium = l_new_premium_to_add
        }
      else
        {
          l_premium = l_premium
          l_save_premium = l_current_premium_to_add + l_premium
        }
  }
else
  if l_save_annual_prior = l_annual_premium then
    {
      l_premium = 0
      l_save_premium = 0
    }
  else
    if l_premium >= l_current_premium_to_add then
      {
        l_new_premium_to_add = l_premium - l_current_premium_to_add
        l_save_premium = l_premium
        l_premium = l_new_premium_to_add
      }
END

