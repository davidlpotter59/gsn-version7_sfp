procedure check_difference_between_files

BEGIN
  access axis_master_premium,
     set axis_master_premium:company_id = "", approximate

  while axis_master_premium:errornumber = 0
    {
      if ((axis_master_premium:trans_date < l_starting_date and
           axis_master_premium:trans_eff => l_starting_date and
           axis_master_premium:trans_eff <= l_ending_date) or
          (axis_master_premium:trans_date => l_starting_date and
           axis_master_premium:trans_date <= l_ending_date and
           axis_master_premium:trans_eff <= l_ending_date)) and
           axis_master_premium:line_of_business one of 7 then
        {
          access axis_master_compare,
             set axis_master_compare:company_id       = l_company_id,
                 axis_master_compare:policy_no        = axis_master_premium:policy_no,
                 axis_master_compare:pol_year         = axis_master_premium:pol_year,
                 axis_master_compare:end_sequence     = axis_master_premium:end_sequence,
                 axis_master_compare:line_of_business = axis_master_premium:line_of_business,
                 axis_master_compare:lob_subline      = axis_master_premium:lob_subline,
                 axis_master_compare:trans_date       = axis_master_premium:trans_date,
                 axis_master_compare:trans_code       = axis_master_premium:trans_code, exact

          if axis_master_compare:company_id       = l_company_id and
             axis_master_compare:policy_no        = axis_master_premium:policy_no and
             axis_master_compare:pol_year         = axis_master_premium:pol_year and
             axis_master_compare:end_sequence     = axis_master_premium:end_sequence and
             axis_master_compare:line_of_business = axis_master_premium:line_of_business and
             axis_master_compare:lob_subline      = axis_master_premium:lob_subline and
             axis_master_compare:trans_date       = axis_master_premium:trans_date and
             axis_master_compare:trans_code       = axis_master_premium:trans_code then
            {
               if axis_master_compare:premium <> axis_master_premium:premium then
                 {
                   access axis_master,
                      set axis_master:policy_no        = axis_master_compare:policy_no,
                          axis_master:pol_year         = axis_master_compare:pol_year,
                          axis_master:end_sequence     = axis_master_compare:end_sequence,
                          axis_master:prem_no          = axis_master_compare:prem_no,
                          axis_master:build_no         = axis_master_compare:build_no,
                          axis_master:line_of_business = axis_master_compare:line_of_business,
                          axis_master:lob_subline      = axis_master_compare:lob_subline,
                          axis_master:line_type        = axis_master_compare:line_type,
                          axis_master:sub_code         = axis_master_compare:sub_code, exact

                   if axis_master:policy_no        = axis_master_compare:policy_no and
                      axis_master:pol_year         = axis_master_compare:pol_year and
                      axis_master:end_sequence     = axis_master_compare:end_sequence and
                      axis_master:prem_no          = axis_master_compare:prem_no and
                      axis_master:build_no         = axis_master_compare:build_no and
                      axis_master:line_of_business = axis_master_compare:line_of_business and
                      axis_master:lob_subline      = axis_master_compare:lob_subline and
                      axis_master:line_type        = axis_master_compare:line_type and
                      axis_master:sub_code         = axis_master_compare:sub_code and
                      axis_master:line_of_business one of 7 then
                     {
                       if axis_master:mark_as_deleted_location one of 1 then
                         {
                           l_diff_total = axis_master:premium + (axis_master_premium:premium - axis_master_compare:premium)

                         }
                       else
                         {
                           l_diff_total = axis_master_premium:premium - axis_master_compare:premium
                         }
                       change axis_master
                         begin
                           if axis_master:mark_as_deleted_location one of 1 then
                             {
                               axis_master:premium = l_diff_total
                             }
                           else
                             {
                               axis_master:premium = axis_master:premium + l_diff_total --axis_master_compare:premium + l_diff_total
                             }
                         end
                     }  --if validation of the access make sure it has a good read on the file before you change.
                 }  --if axis_master_compare:premium <> axis_master_premium:premium
            } -- if statement
        }
      next axis_master_premium
    }
END

